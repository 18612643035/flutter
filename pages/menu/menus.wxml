<!--pages/menu/menus.wxml-->
<scroll-view scroll-y class="content flex-wrap flex-direction-col">
      <van-empty  style="display: {{empty}};" description="暂无操作权限" />
      <view wx:for="{{dataList}}" bindtap="selectBt" class="flex-wrap flex-direction-row">
        <text bindtap="goMenu" data-src="{{item.id}}"  class="tapNav food-name">{{item.name}}</text>
      </view>
</scroll-view>