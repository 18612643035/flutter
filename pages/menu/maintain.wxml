<view class='subBtnBox'>
    <!-- <button class='subBtn' type='button' bindtap='subFormData'>安装完成</button> -->
    <view class="">安装完成</view>
</view>
<view class="query">
  <view bindtap='goFileup' class="optBt">
    <van-icon color="#fff" name="plus" />
  </view>
  <View>
    <span data-index="{{bindex}}">客户名称:</span><text>{{dict[allData.customerId]}}</text>
  </View>
  <View>
    <span data-index="{{bindex}}">经销商:</span><text>{{allData.deptName}}</text>
  </View>
  <view>
    <span data-index="{{bindex}}">科室名称:</span><text>{{allData.deptName}}</text>
  </view>
  <view>
    <span data-index="{{bindex}}">设备类型:</span><text>{{devices[allData.deviceType]}}</text>
  </view>
	<view>
	  <span data-index="{{bindex}}">详细地址:</span><text>{{allData.address}}</text>
	</view>
  <view>
    <span data-index="{{bindex}}">设备数量:</span><text>{{allData.devices.length}}</text>
  </view>
  <view>
    <span data-index="{{bindex}}">信息科联系人:</span><text>{{allData.contact}}</text>
  </view>
  <view>
    <span data-index="{{bindex}}">信息科联系电话:</span><text>{{allData.contactPhone}}</text>
  </view>
	<view>
	  <span data-index="{{bindex}}">安装完成时间:</span><text>{{allData.actualTime}}</text>
	</view>
	<view>
	  <span data-index="{{bindex}}">维保创建时间:</span><text>{{allData.createTime}}</text>
	</view>
	<view>
	  <span data-index="{{bindex}}">问题描述:</span><text>{{allData.content}}</text>
	</view>
	<logBox  http="{{http}}"></logBox>
</view>
