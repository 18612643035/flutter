</view> -->
<view bindtap='goFileup' class="optBt">
    <!-- <text  class='iconfont icon-icon_common_add'></text> -->
  <van-icon color="#fff" name="plus" />
</view>
<van-notify id="van-notify" />
<view class="container" wx:for="{{allData}}" wx:for-index="key" wx:for-item="db">
      <view class="userContext">
        <view class="topBox">
          <view class="userImg">
            <image src="{{db.userAvatar}}"></image>
          </view>
          <view class="orgName">
            <view class="user" wx:if="{{db.userName!=null}}">
              <text>{{db.userName}}</text>
            </view>
            <view>
              <text class="listTitle">{{db.orgName}}</text>
            </view>
          </view>
        </view>
        <view wx:if="{{db.content!=null}}">
          <text>{{db.content}}</text>
        </view>
        <view class="uploadimgbox">
          <view>
            <view wx:for="{{db.images}}" wx:key="item">
              <image class='uploadimg' src='{{item.url}}' data-presrc="{{item.url}}" bindtap='previewImg'
                mode='aspectFill'></image>
            </view>
            <view wx:for="{{db.videos}}" wx:key="item">
              <video class='uploadimg' src="{{item.url}}" controls></video>
            </view>
          </view>
        </view>
        <view wx:if="{{db.canDelete}}" class="vlog_flex">
          <text class="listTitle">{{db.time}}</text>  <text data-id="{{db.id}}" bindtap="onRmove"> 删除</text>
        </view>
      </view>
</view>