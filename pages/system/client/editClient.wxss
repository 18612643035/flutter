
.items{
  border: 1px dotted #000;
  margin: 5px;
}
.items view{
  display: block;
  width: 100%;
  display: flex;
  flex-direction: row;
  margin: 10px 0;
}
.items view input{
  border-radius: 15px;
  border: 1px solid #d2d2d2;
  padding-left: 10px;
}
.items view text{
  width: 25%;
  margin-left: 10px;
  margin-right: 10px;
  text-align: right;
}