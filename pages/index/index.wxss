 .optName{
    width: 200rpx;
    color: #333;
}
.optClass .optName text,.optClass .mesTit text{
    color: #ce0a0a;
}
.optClass .optContent{
    flex: 1;
    text-align: right;
    color: #666;
    font-size:24rpx;
    color:#999;
}

.optClass .uploadimgbox{
    flex: 0 0 33.333%;
    position: relative;
    box-sizing: border-box;
    padding: 10rpx;
    margin-bottom: 10rpx;
}
.optClass .uploadimgbox .progress{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    background-color: rgba(0, 0, 0, .5);
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    line-height: 208rpx;
    z-index: 99;
}
.optClass .uploadimgbox .delbtn{
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    top: 10rpx;
    right: 10rpx;
    background-color: rgba(0, 0, 0, .5);
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    line-height: 50rpx;
    z-index: 98;
}
.optClass .uploadimg,.optClass .uploadbtn{
    display: block;
    width: 100%;
    height: 208rpx;
}
.subBtnBox{
    bottom: 0;
    left: 0;
    width: 100%;
}
view.van-tab{
	margin: 0;
	padding: 0;
	border: 0 !important;
	color: #838286 !important;
	font-size: 26rpx !important;
	border-top-left-radius: 20rpx;
	border-top-right-radius: 20rpx;
}
view.van-tabs__wrap,.van-tabs__nav--card{
	height: 80rpx!important;
}
scroll-view.van-tabs__scroll--card{
	border: none;
	overflow: hidden;
    background: #F5F7FB;
    margin: 0!important;
    width: 100%;
}
view.van-tab--active{
	background: #fff!important;
	color: #FF6D3B !important;
	font-size: 30rpx !important;
	border-top-left-radius: 20rpx;
	border-top-right-radius: 20rpx;
}
.van-field__placeholder{
    color: #838286!important;
		font-size: 24rpx;
}
.van-field__control{
	color: #272634;
	font-size: 26rpx;
}
.menuBox .van-icon{
    color: #838286;
    width: 30rpx;
    height: 30rpx;
}
.menuBox{
    width: 710rpx;
    margin: 0 auto;
		background: #FFFFFF;
		box-shadow: 0rpx 8rpx 8rpx 1rpx rgba(113,106,105,0.04);
		border-radius: 0rpx 20rpx 20rpx 20rpx;
}
view.van-search{
	background-color: #F5F7FB !important;
	height: 70rpx;
	border-radius: 35rpx;
	overflow: hidden;
	margin: 30rpx;
}
.van-search__content{
	background-color: #F5F7FB !important;
}
.van-field__control{
    margin-left: 29rpx !important;
    font-size: 24rpx;
}
.btRow{
	display: flex;
	margin-top: 39rpx;
	flex-flow:row wrap;
	align-content: flex-start;
}
.btRow>view{
	width: 260rpx;
	padding: 0 20rpx;
	height: 120rpx;
	border-radius: 10rpx;
	border: 1px solid rgba(255,109,59,0);
	font-size: 24rpx;
	background-color: #F7F7F7;
	color: #838286;
	display: flex;
	align-items: center;
	margin-left: 31rpx;
	margin-bottom: 39rpx;
	background-image: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 64rpx 62rpx;
	background-position: 38rpx 29rpx;
}
.btRow>view text{
	margin-left: 128rpx;
	display: block;
	width: 142rpx;
}
.vanTab{
	height: 600rpx;
	overflow: auto;
}
.btRow>view.dealerCur{
	background-color: #FFF3EA;
	border: 1px solid rgba(255,109,59,0.73);
	position: relative;
}
.btRow>view.dealerCur::after{
	content: '';
	width: 42rpx;
	height: 36rpx;
	position: absolute;
	right: 0;
	top:0;
	background-size: 100% 100%;
	background-image: url(data:image/png;base64,);
}
.van-ellipsis{
	height: 80rpx;
	line-height: 80rpx;
}
.van-dropdown-menu__title{
	color: #838286 !important;
	font-size: 26rpx !important;
}
.van-dropdown-item__option--active .van-dropdown-item__title{
	color: #FF6D3B !important;
}
.van-cell{
	font-size: 26rpx!important;
	color: #838286 !important;
}
.color1{
	colro:#838286;
}
.color2{
	color:rgba(104, 103, 110, 0.8);
	font-size: 26rpx;
}
.addInput{
	color:#838286;
	padding-left: 67rpx;
	margin-top:39rpx;
	margin-bottom:35rpx ;
	font-size: 26rpx;
}
.van-dropdown-menu{
	height: 80rpx!important;
}