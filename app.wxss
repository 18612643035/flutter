
@media only screen and (device-width : 375px) and (device-height : 603px) and (-webkit-device-pixel-ratio : 2) {
  page{
    font-size: 30rpx;
  }
}
@media only screen and (device-width: 414px) and (-webkit-device-pixel-ratio: 3) {
  page{
    font-size: 30rpx;
  }
}
@media only screen and (device-width: 750px) and (-webkit-device-pixel-ratio: 2) {
  page{
    font-size: 15rpx;
  }
}
/**app.wxss**/
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
}
page{
  background:#f8f8f8;
	font-family: 'PingFang-Regular', sans-serif;
}
.area {
  width: 1%;
	right: 0;
	top: 10%;
  background: rgb(0,0,0,0);
	position: fixed;
	height: 70%;
}
.optBt{
  float: right;
  display: flex;
  margin-right: 32rpx;
  border-radius: 80rpx;
  width: 90rpx;
  height: 90rpx;
  justify-content: center;
  font-size:50rpx;
  z-index: 999;
  background: linear-gradient(0deg, #FD644F, #F28E26);
}
.optBt .van-icon{
  font-size: 40rpx;
}
view.van-switch__node{
  font-size: 24px;
}
.flex-wrap{
  display: flex;

}
.flex-direction-col{
   flex-direction:column;
}
.flex-direction-row{
   flex-direction:row;
}
.button-box{
  display: flex;
  padding-top: 10rpx;
  /* justify-content: center; */
  justify-content: space-evenly;
}
.pop-beg{
  padding: 0 5rpx;
}
.pop-beg>view{
  display: flex;
  flex-direction: row;
}
.pop-beg>input{
  border-bottom: 1rpx solid rgb(133, 133, 133);
  margin-left: 1rpx;
}
.select-wrap{
  width: 50%;
}
.datetime-picker{
  width:100%
}
view.van-picker__toolbar{
  display: none;
}
.items>view{
  width: 100%;
}

.tabRow{
  display: flex;
  flex-direction: row;
  margin-top: 10rpx;
}
.tabRow>text{
  width: 30%;
  text-align: right;
  margin-left: 5rpx;
}
.tabRow>input,.query>view input{
  border: 1px solid #ebeef5;
  color: #606266;
  margin-left: 1rpx;
  padding-left: 5rpx;
  border-radius: 10rpx;
}
.details .query
{
  height: 15rpx;
  overflow: auto;
}
.query>view{
  padding-left: 5rpx;
  display: flex;
  flex-direction: row;
}
.query>view text{
  color: #606266;
}
.query>view span{
  width: 30%;
  text-align: right;
  margin-left: 5rpx;
  display: block;
}
.bt_box{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.bt_box button{
  width: 170rpx;
  font-size: 30rpx;
  line-height: 60rpx;
  margin-top: 2rpx;
}
.dialog{
  max-height: 300rpx;
  overflow: auto;
}
.vsearch{
  display: flex;justify-content: center;align-items: center;position: relative;
}
.searchCont{
  max-height: 500rpx;
  overflow-y: auto;
	line-height: 36rpx;
	margin-left: 67rpx;
	color: #272634;
}
.searchCont>view{
	margin: 30rpx auto;
}
.searchCont>view:first-child{
  margin-top: 0px !important;
}
.dropdown{
  margin-left: 15rpx;
  width:100rpx;
  height: 30rpx;
}
.link{
  border-bottom: 1rpx solid #1989fa;
  width: auto;
}
.vsearch>view{
  width: 80%;
  position: relative;
}
.van-icon{
  color: #1989fa;
}
.query .query{
  display: block;
}
.layOutend{
  display: flex;
  justify-content: space-between;
}
.imgBOx{
	display: flex;
}
.query .trainContain{
	flex-direction: column;
}
.imgContent{
  overflow: hidden;
  width: 662rpx;
  margin: 20rpx auto;
  padding: 0 24rpx;
  padding-top: 20rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 8rpx 8rpx 1rpx rgba(113,106,105,0.02);
  border-radius: 10rpx;
}
.logContent{
  color:rgba(104, 103, 110, 0.8);
  font-size:24rpx;
  line-height: 36rpx;
}
.moreList{
  width: 710rpx;
  margin: 20rpx auto;
  box-shadow: 0rpx 8rpx 8rpx 1rpx rgba(113,106,105,0.04);
  border-radius: 10rpx;
  background: #fff;
	padding-bottom: 30rpx;
  overflow: hidden;
}
.moreList>view{
  display: flex;
  margin:28rpx 0;
  line-height: 25px;
}
.moreList>view span{
  color: rgba(104, 103, 110, 0.8);
  font-size: 24rpx;
  width: 238rpx;
  margin-left: 69rpx;
  display: block;
}
.moreList>view text{
  color: #272634;
  font-size: 24rpx;
  font-weight: bold;
}
.subBtnBox{
  position: fixed;
  background: #fff;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 150rpx;
  z-index: 999;
  box-shadow: 0rpx -8rpx 18rpx 2rpx rgba(255,109,59,0.16);
}
.subBtnBox>view{
  width: 680rpx;
  height: 98rpx;
  margin: 0 auto;
  margin-top: 26rpx;
  background: linear-gradient(90deg, #F28E26, #FD644F);
  border-radius: 49rpx;
  text-align: center;
  line-height: 98rpx;
  color: #fff;
  font-size: 36prx;
}
.subBtnBox>button{
  width: 680rpx;
  height: 98rpx;
  margin: 0 auto;
  margin-top: 26rpx;
  background: linear-gradient(90deg, #F28E26, #FD644F);
  border-radius: 49rpx;
  text-align: center;
  line-height: 98rpx;
  color: #fff;
  font-size: 36prx;
}
.more-btmBox{
  height: 170rpx;
}
.vlog_flex{
  margin-bottom: 29rpx;
	display: flex;
	justify-content: space-between;
}
.delete{
	width:32rpx;
	height: 30rpx;
}
.btmBox{
  font-size: 20rpx;
  font-weight: 400;
  color: rgba(104, 103, 110, 0.8);
  line-height: 28rpx;
}
.uploadimgbox view>view{
  width: 200rpx;
  height: 150rpx;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
  overflow: hidden;
}
.uploadimgbox{
  margin-top: 23rpx;
}
.uploadimgbox>view{
  display: flex;
  overflow: hidden;
  flex-wrap: wrap;
  justify-content: space-between;
}
.uploadimgbox>view:after{
  content: "";
  width: 200rpx;
  overflow: hidden;
}
.uploadimg,.uploadimgbox .file{
  width: 100%;
  height: 100%;
}
.van-uploader__upload{
	width: 160rpx !important;
	height: 160rpx !important;
	border-radius: 10rpx !important;
	overflow: hidden;
	margin: 0!important;
	padding: 0 !important;
	background-color: #F1F1F1!important;
}
.van-uploader__preview{
	margin: 0 28rpx 28rpx 0!important;
}
.van-uploader__upload .van-icon{
	color: #B4BAC3;
}
.van-uploader__upload .van-icon::before{
	font-size: 96rpx;
}
/* 上传样式 */
.upImgtext{
	width: 710rpx;
	background: #FFFFFF;
	border-radius: 10rpx;
	margin: 20rpx auto;
	overflow: hidden;
	box-shadow: 0rpx 8rpx 8rpx 1rpx rgba(113,106,105,0.04);
}
.optClass{
  min-height: 140rpx;
    padding:30rpx;
    padding-bottom: 20rpx;
	flex-wrap:wrap;
}
.optImage{
    margin: 0 30rpx;
		margin-bottom: 30rpx;
}
.describe{
    font-size: 26rpx;
    margin: 0;
    display: block;
		color: #838286;
}
.van-icon-browsing-history{
    margin-left:10rpx;
}
.trainContain{
  overflow:hidden
}